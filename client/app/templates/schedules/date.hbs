{{outlet}}
<form class="ui form">

  <div class="field">
    <label>Start date:</label>
    {{pikaday-input value=startsAt minDate=minDate theme="dark-theme"}}
  </div>
  
  {{sit-select
    label='Sitting for Who'
    prompt='Select a Cleint'
    content=model.clients
    optionValue='id'
    optionLabel='familyName'
    value=familyName
    isEditing=true
    name="client"
    onSelect='clientSelected'}}
</form>

{{link-to 'Continue' 'schedules.start-time' class='ui primary button'}}
