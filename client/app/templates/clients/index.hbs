<h1>Clients</h1>

<div class="ui relaxed divided list">
  {{#each model as |client|}}
    <div class="item">
      <div class="content">
        {{link-to client.familyName clients.client class='header'}}
        <div class="description">phone: {{client.primaryPhone}}</div>
      </div>
    </div>
  {{/each}}
</div>

{{link-to 'Add New Client' 'clients.new' class="ui primary button"}}
